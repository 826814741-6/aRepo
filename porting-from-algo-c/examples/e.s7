(load "src/e.s7")

(call-with-values
  e
  (lambda (r n)
    (format #t "   ~~E: ~E (~D)~%" r n)
    (format #t "   ~~F: ~F~%" r)
    (format #t "   ~~G: ~G~%" r)
    (format #t "   ~~A: ~A~%" r)
    (format #t "~~,20E: ~,20E~%" r)
    (format #t "~~,20F: ~,20F~%" r)
    (format #t "~~,20G: ~,20G~%" r)))
